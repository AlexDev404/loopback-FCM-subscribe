{
  "name": "ub-loopback",
  "version": "0.0.1",
  "description": "ub loopback",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "src/index.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\"",
