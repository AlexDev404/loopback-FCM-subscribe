    "openapi-spec": "node ./dist/openapi-spec",
    "start": "tsx .",
    "dev": "nodemon .",
    "prepare": "npm run build",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git"
  },
  "author": "AlexDev404 <AlexDev404@users.noreply.github.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^6.1.4",
    "@loopback/core": "^5.1.4",
    "@loopback/rest": "^13.1.4",
    "@loopback/rest-explorer": "^6.1.4",
    "firebase-admin": "^12.0.0",
    "tslib": "^2.0.0",
    "tsx": "^4.6.2"
  },
  "devDependencies": {
    "@loopback/eslint-config": "^14.0.5",
    "@types/node": "^16.18.68",
    "nodemon": "^3.0.2",
    "prettier": "^3.0.3",
    "typescript": "~5.2.2"
  }
